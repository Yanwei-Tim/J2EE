	public static int getWhole_len(byte[] buf){
		byte[] whole_len = new byte[InsideMsg.MSGLENSIZE];
		System.arraycopy(buf, 0, whole_len, 0, InsideMsg.MSGLENSIZE);
		return Integer.parseInt(new String(whole_len));
	}
	
	/**
	 * 根据不同字段的类型取出相应长度的内容，返回已经取出字节流的总长度
	 * 
	 * @param length
	 * @param buf
	 * @param location
	 * @param bytevalue
	 * @return
	 */
	public static int setBytevalue(int length, byte[] buf, int location,
			byte[] bytevalue, String cmd) {
		System.arraycopy(buf, location, bytevalue, 0, length);
		location += length;
		return location;
	}
	
	
	public static String checkLength(String param,int len){
		if(param.length()>len){
			return param.substring(0,len);
		}else{
			return param;
		}
	}
