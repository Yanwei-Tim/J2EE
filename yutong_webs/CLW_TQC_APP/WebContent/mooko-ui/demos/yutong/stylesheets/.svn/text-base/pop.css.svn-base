html, body{ margin:0; padding:0; border:none; font:12px/18px  'Microsoft Yahei','微软雅黑',Arial,Verdana;}
ul{margin:0;padding:0;}
img{border:0;margin:0;padding:0;}
#popArea {
	height: 540px;
	width: 500px;
	display:block;
	border:1px solid #999999;
	position:fixed;
	top:0;
	right:0;
	background:#eeeeee;
}
#popArea .pop-tools{
	height: 56px;
	display:block;
	background:url(../images/pop_title.png) repeat-x;
}
#popArea .pop-btn{
	height: 54px;
	width: 21px;
	display:block;
	float:left;
}

#popArea h1{
	font-size:18px;
	color:#333333;
	font-weight:normal;
	float:left;
	line-height:30px;
	margin:12px 10px;
}
#popArea ul{
	float:right;
	line-height:50px;
	margin-top:5px;
	margin-right:5px;
	list-style:none;
}
#popArea ul li{
	width:46px;
	height:45px;
	display:block;
	float:left;
	margin:0 2px;
}
#popArea li.selected{
	background: url(../images/pop_btn_bg.png) no-repeat;
}
#popArea li img{
	margin:8px 0 0 8px;
}

#popArea .warn{
	color:#333333;
	background:#eeeeee;
	padding:10px 10px 0 10px;
	position:relative;
	width:480px;
	border-bottom:1px solid #999999;
}
#popArea h2{
	font-size:14px;
	float:left;
	margin:0;
	font-weight:bold;
}
#popArea .times{
	font-size:12px;
	font-weight:normal;
	margin-left:20px;
}
#popArea .wait{
	width:150px;
	position:absolute;
	right:10px;
	top:10px;
	text-align:right;
	font-weight:bold;
}
#popArea .wait strong{
	color:#0078c8;
	font-size:18px;
	font-weight:bold;
	margin-right:5px;
}
#popArea .table{
	margin:10px 0;
}

#popArea .table tr{
	line-height:25px;
	height:25px;
	vertical-align:middle;
}
#popArea .table th{
	font-weight:bold;
}
#popArea .table td{
	width:100px;
	background: url(../images/pop_read_bg_1.png) no-repeat;
	padding-left:5px;
}
#map{
	height:280px;
	background:#cccccc;
}
#popArea .sub-form{
	padding:10px;
	border-top:1px solid #999999;
}
#popArea .sub-form textarea{
	width:470px;
	border-top:1px solid #8f8f8f;
	border-left:1px solid #cecece;
	border-right:1px soiid #cecece;
	border-bottom:1px solid #e4e4e4;
	margin-bottom:10px;
}
.sub-form a{
	color:#0078c8;
	text-decoration:none;
}
.warn-history{
	background: url(../images/warn_h.png) no-repeat;
	padding:5px 5px 5px 35px;
	float:left;
}
.btn-blue{
	border:0;
	background: url(../images/btn_blue.gif) no-repeat;
	width:76px;
	height:28px;
	float:right;
	font-size:14px;
	color:#ffffff;
}