.refreshAndreset2{ background: #fff  repeat-x left top; float:left; padding-left:25px;border-bottom: 0px solid #b8b8b8;height: 35px; line-height: 35px; padding-top: 12px; width: 89%;}

.refreshAndreset{
	
	background:#eeeeee;
	height:39px;
	position:relative;
}

.refreshAndreset2 a.btnRefresh{ color: #2a2a2a; display: block; height: 25px; float: left; line-height: 25px; margin:0 5px; padding-left: 34px; text-decoration: none; width: 48px; }
.refreshAndreset2 a.btnReset{ color: #2a2a2a; display: block; height: 25px; float: left; line-height: 25px; margin:0 5px; padding-left: 34px; text-decoration: none; width: 48px; }
a.btnRefresh{ background: url(../newimages/qipaoimages/btn_refresh.png) no-repeat left center; }
a.btnReset{ background: url(../newimages/qipaoimages/btn_reset.png) no-repeat left center; }

