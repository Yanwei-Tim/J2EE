@CHARSET "UTF-8";
/* CSS Document */


/* 弹出气泡  样式开始*/
.balloon{position:absolute; height: 204px; width: 300px;_width:326px;}
.balloon ul, .balloon li{ list-style: none; margin: 0;}
.balloon .balloonLeft{
	background:url(../newimages/qipaoimages/balloons.gif) no-repeat left top;
	float: left;
	height: 204px;
	width: 10px;
}
.balloon .balloonRight{
	background:url(../newimages/qipaoimages/balloons.gif) no-repeat right top;
	float: left;
	height: 204px;
	width: 10px;
}
.balloon .balloonMain{
	background:url(../newimages/qipaoimages/balloons_bg.gif) repeat-x right top;
	float: left;
	height: 204px;
	width: 280px;
}
.balloon .balloonTil{
	color: #222;
	float: left;
	height: 42px;
	width: 100%;
}
.balloon a.tabfocus{
	background: url(../newimages/qipaoimages/balloons_tab.gif) no-repeat left center;
	color: #fff;
	display: block;
	float: left;
	line-height: 42px;
	margin-right: 10px;
	text-align: center;
	text-decoration: none;
	width: 70px;
}
.balloon a:hover.tab{ text-decoration: underline;}
.balloon a.tab{
	color: #222;
	display: block;
	float: left;
	line-height: 42px;
	margin-right: 10px;
	text-align: center;
	text-decoration: none;
	width: 70px;
}
.balloon a.close{
	background: url(../newimages/qipaoimages/balloons_close.gif) no-repeat left top;
	display: block;
	float: right;
	height: 16px;
	margin-top: 13px;
	width: 16px;
}
.balloon .data{ border-bottom: 1px solid #ccc; float: left;_float: ; height: 102px; padding-top: 5px; width: 100%;}
.balloon .toolbarqp{ border-top: 1px solid #fff; float: left;_float: ;width: 280px; height: 44px; text-align: center; padding: 0 0 0 0px;}
a.tool1, a.tool2, a.tool3,a.tool3h, a.tool4,a.tool4h, a.tool5, a.tool6, a.tool7,a.tool7h{ cursor: pointer; display: block; float: left; height: 30px; margin: 6px 6px 4px 6px; width: 40px; }
.balloon a.tool1{ background: url(../newimages/qipaoimages/btn_tool5.png) no-repeat center top;}
.balloon a.tool2{ background: url(../newimages/qipaoimages/btn_tool6.png) no-repeat center top;}
.balloon a.tool3{ background: url(../newimages/qipaoimages/btn_tool3.png) no-repeat center top;}
.balloon a.tool3h{ background: url(../newimages/qipaoimages/btn_tool3h.png) no-repeat center top;}
.balloon a.tool4{ background: url(../newimages/qipaoimages/btn_tool2.png) no-repeat center top;}
.balloon a.tool4h{ background: url(../newimages/qipaoimages/btn_tool2h.png) no-repeat center top;}
.balloon a.tool5{ background: url(../newimages/qipaoimages/btn_tool1.png) no-repeat center top;}
.balloon a.tool6{ background: url(../newimages/qipaoimages/ico_warning.png) no-repeat center top;}
.balloon a.tool7{ background: url(../newimages/qipaoimages/btn_tool7.png) no-repeat center top;}
.balloon a.tool7h{ background: url(../newimages/qipaoimages/btn_tool7_1.png) no-repeat center top;}
.balloon .arrow{ background:url(../newimages/qipaoimages/balloons_arrow.gif) no-repeat top center; float: left; height: 9px; width: 100%;}


.toolbarqp2{ float: right;border-top: 1px solid #fff;  height: 35px; text-align: center; padding: 0 0 0 2px;}
.toolbarqp2 a.tool1{ background: url(../newimages/qipaoimages/btn_tool5.png) no-repeat center top;}
.toolbarqp2 a.tool2{ background: url(../newimages/qipaoimages/btn_tool6.png) no-repeat center top;}
.toolbarqp2 a.tool5{ background: url(../newimages/qipaoimages/btn_tool1.png) no-repeat center top;}
.toolbarqp2 a.tool6{ background: url(../newimages/qipaoimages/ico_warning.png) no-repeat center top;}
.toolbarqp2 a.tool1, .toolbarqp2 a.tool2,.toolbarqp2 a.tool5,.toolbarqp2 a.tool6{ cursor: pointer; display: block; float: left; height: 30px; margin: 6px 6px 4px 6px; width: 30px; }

