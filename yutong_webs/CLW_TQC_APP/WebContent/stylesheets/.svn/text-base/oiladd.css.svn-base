.button_green {
	width:80px;
	height:26px;
}
.button_green a {
	display:block;
	color:#fff;
	text-align:center;
	height:26px;
	line-height:26px;
	background:url(../images/kcptImages/button.png) -100px 0px no-repeat;
}
.button_green a:hover {
	background:url(../images/kcptImages/button.png) -100px -50px no-repeat;
}
.button_green a:active {
	background:url(../images/kcptImages/button.png) -100px -100px no-repeat;
}
.divbackimgcheckboxcheck{
	background:url(../scripts/plugins/ligerUI/skins/Aqua/images/tree/tree.png) -22px -220px;
