h1,h2,h3{
font-size: 14px;
}

h4{
font-size: 14px;
}

.noticeMsg{
font-size: 12px;
color: #CC0000;
padding: 2px;
}

.error{
font-size: 12px;
color: #CC0000;
}

.success{
font-size: 12px;
color: #009900;
}

div#message, .message{
clear: both;
text-align: center;
list-style-type: square;
overflow: hidden;
}


