package com.neusoft.clw.common.exceptions;

/**
 * @author <a href="mailto:jin.p@neusoft.com">jinp </a>
 * @version $Revision 1.1 $ 2011-2-28 02:44:51
 */
public class BusinessException extends Exception {

    private static final long serialVersionUID = -395519744859162811L;

    public BusinessException() {
        super();
    }

    public BusinessException(Exception e) {
        super(e);
    }

    public BusinessException(String msg) {
        super(msg);
    }

    public BusinessException(String msg, Throwable ex) {
        super(msg, ex);
    }
}
