<struts>
	<!-- 终端管理【新】 -->
	<package name="zdnew" namespace="/zdnew" extends="clw-default">
		<!-- 终端参数管理 -->
		<!-- 初始化 -->
		<action name="init" class="newTerminalParamsAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
		</action>
		
		<action name="vehicleupdown_init" class="newTerminalParamsAction" method="vehicleupdown_init">
			<result name="success">
				/WEB-INF/jsp/zdnew/updownmonitoring.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/updownmonitoring.jsp
			</result>
		</action>
		<action name="update" class="newTerminalUpdateAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalRomoteUpdate.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalRomoteUpdate.jsp
			</result>
		</action>
		
		<action name="updateInfo" class="newTerminalUpdateAction" method="insert">
		</action>
		
		<action name="setTerminalParams" class="newTerminalParamsAction" method="setTerminalParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
		</action>
		
		<action name="queryTerminalParams" class="newTerminalParamsAction" method="queryTerminalParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
		</action>
		
		<action name="refreshTerminalParams" class="newTerminalParamsAction" method="refreshTerminalParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalParams.jsp
			</result>
		</action>
		
		
		<!-- 终端版本信息 -->
		<action name="version_init" class="newTerminalVersionsAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/terminalVersions.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/terminalVersions.jsp
			</result>
		</action>
		
		<!-- 车速监控 -->
		<action name="speedmonitoring_init" class="speedMonitoringAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/speedmonitoring.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/speedmonitoring.jsp
			</result>
		</action>
		<!-- 开启vss矫正 -->
		<action name="startAdjust" class="speedMonitoringAction" method="startAdjust">
		</action>
		<!-- 关闭vss矫正 -->
		<action name="closeAdjust" class="speedMonitoringAction" method="closeAdjust">
		</action>
		
		<!-- 终端设备状态管理 -->	
		<action name="device_init" class="newTerminalDeviceStatusAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/deviceStatus.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/deviceStatus.jsp
			</result>
		</action>		
		
		<!-- 油量监控参数设置 -->		
		<action name="ftlyinit" class="ftlyParamAction" method="blankAction">
			<result name="success">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
		</action>
		<action name="setFtlyParams" class="ftlyParamAction" method="setFtlyParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
		</action>
		<action name="queryFtlyParams" class="ftlyParamAction" method="queryFtlyParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
		</action>
		<action name="refreshFtlyParams" class="ftlyParamAction" method="refreshFtlyParams">
			<result name="success">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
			<result name="error">
				/WEB-INF/jsp/zdnew/zsptFtlyParam.jsp
			</result>
		</action>
	</package>
