package com.yutong.axxc.tqc.entity;

import java.io.Serializable;

import com.fasterxml.jackson.annotation.JsonIgnore;
import com.fasterxml.jackson.annotation.JsonProperty;

public class Advises  implements Serializable{

	private static final long serialVersionUID = -8301612632212283520L;
	
	@JsonIgnore
	private String advice_id;
	
	@JsonProperty("advise")
	private String advise;
	
	@JsonProperty("reply")
	private String reply;
	
	@JsonProperty("advise_time")
	private String advise_time;

	@JsonProperty("reply_time")
	private String reply_time;

	@JsonProperty("msg_id")
	private String msg_id;
	
	
	public String getAdvice_id() {
		return advice_id;
	}

	public void setAdvice_id(String advice_id) {
		this.advice_id = advice_id;
	}

	public String getAdvise() {
		return advise;
	}

	public void setAdvise(String advise) {
		this.advise = advise;
	}

	public String getReply() {
		return reply;
	}

	public void setReply(String reply) {
		this.reply = reply;
	}

	public String getAdvise_time() {
		return advise_time;
	}

	public void setAdvise_time(String advise_time) {
		this.advise_time = advise_time;
	}

	public String getReply_time() {
		return reply_time;
	}

	public void setReply_time(String reply_time) {
		this.reply_time = reply_time;
	}

	public String getMsg_id() {
		return msg_id;
	}

	public void setMsg_id(String msg_id) {
		this.msg_id = msg_id;
	}
	
}
