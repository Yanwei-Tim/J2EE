	<select id="selectCarBaseInfoForExport"  parameterClass="Map" resultMap="data-result-export">
	SELECT t.vehicle_ln,
	       t.vehicle_vin,
	       t.business_type,
	       t.tyre_r,
	       t.rear_axle_rate,
	       t.standard_rotate,
	       t.vehicle_color,
	       t.limite_number,
	       t.dead_load,
	       t.vehicle_number,
	       t.engine_number,
	       t.standard_oil,
	       t.brand,
	       t.out_number,
	       t.sell_order_num,
	       t.made_order_num,
	       t.chassis_order_num,
	       t.sell_date,
	       t.car_number,
	       t.car_type_brand,
	       t.oil_config
	  FROM clw_cl_base_info_t t
	 WHERE t.valid_flag = '0'
	   AND T.DEVICE_TYPE = '0'
	   <isNotEmpty prepend="AND" property="vehicle_vin">
       	t.vehicle_vin LIKE '%$vehicle_vin$%'
       </isNotEmpty>
       <isNotEmpty prepend="AND" property="vehicle_ln">
       	t.vehicle_ln LIKE '%$vehicle_ln$%'
       </isNotEmpty>
	 ORDER BY t.create_time
 	</select>
