.ub_timespan
  {
	border:1px solid #c2d3e5;
	background-color:rgb(246, 246, 246);
	display:block;
	float:left;
  }
  .ub_timespan a
  {
	float:left;
	border-color:#f6f6f6;border-style:solid;border-width:0 1px;padding:4px 6px 2px 6px;height:20px;line-height:18px;
  }
  .ub_timespan .first
  {
	border-left:0px;
  }
  .ub_timespan .last
  {
	border-right:0px;
  }
  .ub_timespan a.selected
  {
	border-color:#c2d3e5;color:#000;font-weight:bolder;
	background-repeat:repeat;background-image:url("../images/flash-indicator.png");
  }
  .ub_timespan a:hover
  { 
	border-color:#c2d3e5;
	background-repeat:repeat;background-image:url("../images/flash-indicator.png");
  }
  .ub_timespan a.disable
  {

	border-color:#f6f6f6;color:#ccc;cursor:default;text-decoration:none;
	background-image:none;
	border-width:0;
  }
TABLE.summary
{
	width:100%;
	border:1px solid #f1f1f1;
	background-color:rgb(253, 242, 239);
	border-collapse:collapse;

}
  TABLE.summary TR TD
  {
  	text-align:left;
  	line-height:1.2;
  	padding:8px 30px;
	border:1px solid #e4e4e4; 
  }
   TABLE.summary TR TD SPAN.text
  {
  	color:#555;
  	line-height:24px;
  }
  TABLE.summary TR TD SPAN.value
  {
  	color:#333;
  	font-size:20px;
  }
  .chartlegent
  {
  	float:right;
  }
  .chartlegent span
  {
  	padding-left:20px;
	background-image:url("../images/dot-red.png");
	background-repeat:no-repeat;
	line-height:16px;
	height:16px;
	vertical-align:middle;
	margin-right:30px;
  }
  .sumtable
{
	border-collapse:collapse;
	cellpadding:0;
