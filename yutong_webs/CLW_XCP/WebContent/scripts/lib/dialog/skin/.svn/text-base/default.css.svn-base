/* artDialog default.css
 * Date: 2013-02-18
 */

.aui_default .aui_dialog{ border:1px #666 solid;}
.aui_default .aui_td_title, .aui_default .aui_td_buttons button {  font:12px 'Microsoft Yahei', Arial; }
.aui_default .aui_content_table { min-width:9em; background:#FFF; }
.aui_default .aui_loading_tip { width:16px; height:16px; margin:-8px 0 0 -8px; text-indent:-9999em; background:url(default/loading.gif) no-repeat center center; }
.aui_default .aui_center { background:#FFF; }
.aui_default .aui_title_wrap { width:100%; height:28px; position:absolute; top:0; left:0; _left:auto; }
.aui_default .aui_title { height:28px; line-height:28px; padding:0 60px 0 12px; _padding:0 0 0 6px; color:#000; font-weight:700; text-shadow:0 1px 0 #fff; }
.aui_default .aui_left_top, .aui_default .aui_right_top, .aui_default .aui_left, .aui_default .aui_right, .aui_default .aui_left_bottom, .aui_default .aui_right_bottom, .aui_default .aui_close { background-image:url(default/default_s.png); background-repeat:no-repeat; }
.aui_default .aui_left_top { width:0px; height:28px; background-position: -46px -8px; }
.aui_default .aui_right_top { width:0px; height:28px; background-position: -53px -8px; }
.aui_default .aui_left { background-position:-60px 0; background-repeat:repeat-y; }
.aui_default .aui_right { background-position:-65px 0; background-repeat:repeat-y; }
.aui_default .aui_left_bottom { width:0px; height:0px; background-position: -46px -2px;}
.aui_default .aui_right_bottom { width:0px; height:0px; background-position: -53px -2px;}
.aui_default .aui_close { top:5px; right:12px; _right:6px; width:16px; height:17px; background-position:0 0; _font-size:0; _line-height:0; text-indent:-9999em; }
.aui_default .aui_close:hover { background-position:0 -18px; }
.aui_default .aui_top, .aui_default .aui_bottom { background-image:url(default/border.png); background-repeat:repeat-x; }
.aui_default .aui_top { background-position:0 top; }
.aui_default .aui_bottom { background-position: 0 bottom; }
.aui_default .aui_buttons_wrap { background-color:#eee; border-top:solid 1px #ccc; padding-right:15px; }
.aui_default .aui_buttons_wrap span, .aui_default .aui_buttons_wrap button { background-color:transparent; /*background-image:url(default/btns.png); background-repeat:no-repeat; */}
.aui_default .aui_buttons_wrap span { padding:0;  /*background-position:left -42px;*/ }
.aui_default .aui_buttons_wrap button { height:21px; line-height:21px; padding:0 5px; max-width:200px; border:1px #000 solid; border-radius:3px; background:-webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc)); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc'); BORDER-LEFT: #1c6a9e 1px solid; BACKGROUND: #2288cc; COLOR: #fff; BORDER-TOP: #1c6a9e 1px solid; BORDER-RIGHT: #1c6a9e 1px solid; text-shadow: -1px -1px 1px #1c6a9e; /*background-position:right 0;*/ }
.aui_default .aui_buttons_wrap span:hover { background-position:left -63px; }
.aui_default .aui_buttons_wrap span:hover button { /*background-position:right -21px;*/ }
html:hover .aui_default .aui_buttons_wrap button:focus { outline:none 0; }

.aui_default .aui_icon { width:48px; height:48px; margin:10px 0 10px 10px; background-image:url(aero/icons.png); background-repeat:no-repeat; }
.aui_default .art_alert { background-position:0 -96px; }
.aui_default .art_succeed { background-position:0 0;}
.aui_default .art_error { background-position:0 -48px; }
.aui_default .art_confirm { margin-right:10px; width:48px; height:48px; background-position:0 -144px; }