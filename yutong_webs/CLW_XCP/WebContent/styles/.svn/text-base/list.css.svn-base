#list table a{ color: #008cdc; display: block; height: 20px; line-height:20px; text-decoration: none; width:100%; }
#list th{ padding: 0 4px; border-right:1px solid #ccc; font-size: 12px; line-height: 22px; text-align: left; font-weight: normal; color: #2a2a2a;}
#list th.rightLine{ padding: 0 4px; font-size: 12px; line-height: 22px; text-align: left; font-weight: normal; color: #2a2a2a; }
#list td{ padding: 2px 4px; font-size: 12px; line-height:20px; background: url(../images/dot.gif) repeat-x left bottom;}
#list td.listRightLine{ border-right: none; }
#list thead{ background: #ccc url(../images/listTitle_bg.gif) repeat-x left top; line-height: 22px;}

#listRoute th{ padding: 0 4px; border-right:1px solid #ccc; font-size: 12px; line-height: 22px; text-align: left; font-weight: normal; color: #2a2a2a;}
#listRoute th.rightLine{ padding: 0 4px; font-size: 12px; line-height: 22px; text-align: left; font-weight: normal; color: #2a2a2a; }
#listRoute td{ padding: 2px 4px; font-size: 12px; line-height:20px; background: url(../images/dot.gif) repeat-x left bottom;}
#listRoute td.listRightLine{ border-right: none; }
#listRoute thead{ background: #ccc url(../images/listTitle_bg.gif) repeat-x left top; line-height: 22px;}

.odd{ background: #f8f8f8;}
