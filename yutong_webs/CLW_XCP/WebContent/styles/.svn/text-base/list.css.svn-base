/*page ctrl*/
#pageCtrl{ 
	color: #2a2a2a;
	float: left;
	height: 32px;
	line-height: 32px;
	width: 100%;
}
#gotoPage ul {
	width: 95%;
	height: 18px;
	list-style: none;
	margin: 0;
	padding: 0;
	margin:0 auto;
}
#gotoPage li {
	/*position:relative; del by sunwei */
	float:left;
	margin:0 6px;
}
#gotoPage a {
	text-decoration:none;
	display:block;
	width:18px;
	height:18px;
	margin-top:5px;
}
a.btnFirst { background: url(../images/btn_ifirst.gif) no-repeat left top;}
a.btnFirst:hover { background: url(../images/btn_ifirst_hover.gif) no-repeat right top;}
a.btnPrev { background: url(../images/btn_iPrev.gif) no-repeat left top;}
a.btnPrev:hover { background: url(../images/btn_iPrev_hover.gif) no-repeat right top;}
a.btnNext { background: url(../images/btn_iNext.gif) no-repeat left top;}
a.btnNext:hover { background: url(../images/btn_iNext_hover.gif) no-repeat right top;}
a.btnEnd { background: url(../images/btn_iEnd.gif) no-repeat left top;}
a.btnEnd:hover { background: url(../images/btn_iEnd_hover.gif) no-repeat right top;}

