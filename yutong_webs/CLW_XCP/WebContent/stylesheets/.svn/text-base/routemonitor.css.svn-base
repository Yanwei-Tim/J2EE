@CHARSET "UTF-8";
/* CSS Document */
html, body{ margin:0; padding:0; border:none; font:12px/18px  'Microsoft Yahei','微软雅黑',Arial,Verdana;}
/*rm为线路监控样式集专属标记*/
.rmMain{ width: 100%;}
/*.rmMain{ float:left; width: 100%;}*/
.rmTitle{ background:#f5f5f5 url(../newimages/rTitle_bg.png) repeat-x left top; border-bottom: 1px solid #ccc;  height: 50px; line-height: 20px; padding: 20px 0 0 0; text-align: center; width: 100%; }
/*.rmTitle{ background:#f5f5f5 url(../newimages/rTitle_bg.png) repeat-x left top; border-bottom: 1px solid #ccc; float: left; height: 50px; line-height: 20px; padding: 20px 0 0 0; text-align: center; width: 100%; }*/

/*.rmRoutePlan{ float: left; height: 14px;width: 100%;padding: 10px 0px 0;}*/
.rmRoutePlan{ width: 100%;padding: 10px 0px 0;}
/*.rmRoutePlan{ float: left; height: 300px; overflow:auto; padding: 10px 30px 0; min-width: 880px; width: auto;}*/
.rmSelectBus{height: 28px; width: 100%; overflow:hidden;}
.rmSelLeft{ float: left; line-height: 22px; width: 250px; margin-left: 30px;}
.rmSelRight{ float: right; line-height: 22px; margin-right: 30px;}
.rmSelRight span{ color: #555; float: left; height: 22px; padding: 0 5px 0 20px; width: 80px; }
.rm_icoCGreen, .rm_icoCBlue, .rm_icoCYellow, .rm_icoCRed, .rm_icoCPur{ height: 22px; }
.rm_icoCGreen{ background: url(../newimages/ico_rGreen.png) no-repeat left center;}
.rm_icoCBlue{ background: url(../newimages/ico_rBlue.png) no-repeat left center;}
.rm_icoCYellow{ background: url(../newimages/ico_rYellow.png) no-repeat left center;}
.rm_icoCRed{ background: url(../newimages/ico_rRed.png) no-repeat left center;}
.rm_icoCPur{ background: url(../newimages/ico_rPurple.png) no-repeat left center;}
.rmRoute{ background: #f5f5f5 url(../newimages/rm_titleBg.png) no-repeat left center; border: 1px solid #ccc; float: left; height: 120px; margin-bottom: 10px; width: 100%; }
.rmRouteTil{ float: left; font-weight: bold; text-align: center; line-height: 120px; width: 60px;}
.rmRouteStation{ float: left; height: 100px; padding: 10px; width: auto;}
.rmRouteStation ul{ float: left;}
.rmRouteStation li{ float: left; height: 100px; text-align: center; width: 140px;}
.rmRouteStation a{ color: #333; text-decoration: none;}
.rmRouteStation a:hover{ color: #0078c8; }
.rmRouteStation li.linebgLef{ background: url(../newimages/lline_lBg.png) no-repeat left center; }
.rmRouteStation li.linebgRig{ background: url(../newimages/lline_rBg.png) no-repeat left center; }
.rmRouteStation li.linebgMid{ background: url(../newimages/lline_mBg.png) no-repeat left center; }
.rmInfo{ float: left; height: 38px; width: 100%;}
.btn_stGreen, .btn_stBlue, .btn_stYellow, .btn_stRed{ float: left; display: block; height: 62px; padding: 28px 10px 0 10px; width: 120px; }
.btn_stGreen{ background: url(../newimages/station_green.png) no-repeat center top; }
.btn_stBlue{ background: url(../newimages/station_blue.png) no-repeat center top; }
.btn_stYellow{ background: url(../newimages/station_yellow.png) no-repeat center top; }
.btn_stRed{ background: url(../newimages/station_red.png) no-repeat center top; }
.rmArrowUp, .rmArrowDown, .rmArrowUp2, .rmArrowDown2{ display: block; font-size: 10px; height: 32px; text-align: center; width: 12px; }
.rmArrowUp{ background: url(../newimages/ico_rArrowUp.png) no-repeat center bottom; float:right; margin-right: 48px; }
.rmArrowDown{ background: url(../newimages/ico_rArrowDown.png) no-repeat center bottom; float: left; margin-left: 48px;  }
.rmArrowUp2{ background: url(../newimages/ico_rArrowUp.png) no-repeat center bottom; float:right; margin-right: 64px; }
.rmArrowDown2{ background: url(../newimages/ico_rArrowDown.png) no-repeat center bottom; float: left; margin-left: 64px;  }
.icoSBBox{ background: url(../newimages/ico_rTip_bg.png) no-repeat center top; text-align: center; height: 40px; width: 72px; position: relative; top: 24px; left: 230px; z-index: 100;}
.rmBusNum{ float: left; line-height: 20px; padding-bottom: 8px; margin-top: -120px; text-align: center; width: 100%;  }
a.rmicoBus{ background: url(../newimages/ico_rBus.png) no-repeat center top; display: block; float: left; height: 12px; text-align: center; margin: -91px 0 0 23px; width: 24px; }

/*.rmInfoPlan{ background: #f5f5f5; border-left: 1px solid #b4b4b4; border-right: 1px solid #b4b4b4; border-bottom: 1px solid #b4b4b4; float: left; margin: 0px 4px 4px 4px; }*/
.rmInfoPlan{ background: #f5f5f5; border:1px solid #b4b4b4; border-top:none; }
.rmInfoPlan a{ text-decoration: none;}
/*.rmInfoPlan table{ margin: 4px;}*/
/*.rmInfoTab{ background: url(../newimages/btn_tab_bg.png) repeat-x left top; float: left; height: 24px; line-height: 24px; width: 100%;}*/
.rmInfoTab{ background: url(../newimages/btn_tab_bg.png) repeat-x left top; overflow:hidden; height: 24px; line-height: 24px; }
.rmInfoTab strong{ padding-left: 10px;}
.rmInfoTab ul{ float: right; line-height: 24px; width: 524px;}
.rmInfoTab li a.rmbusNumber{ border-left: 1px solid #b4b4b4; color: #333; float: right; display: block; line-height: 24px; text-indent: 10px; width:120px;cursor:pointer}
.rmInfoTab li a.rmbusNumberFocus{ background: url(../newimages/btn_tabFocus.png) repeat-x left top; border-left: 1px solid #b4b4b4; color: #fff; display: block; float: right; line-height: 24px; text-indent: 10px; width:120px;cursor:pointer}
.rmTime{ font-size: 10px; padding: 8px;}
.rmInfoTab li a.rmArrowLeft{ background: url(../newimages/btn_tabPrev.png) no-repeat left top; border-left: 1px solid #b4b4b4;  border-right: 1px solid #b4b4b4; display: block; float: left; height: 24px; width:18px;cursor:pointer}
.rmInfoTab li a.rmArrowRight{ background: url(../newimages/btn_tabNext.png) no-repeat left top; border-left: 1px solid #b4b4b4; display: block; float: right; height: 24px; width:18px;cursor:pointer}
.rmInfoContent{ width: 100%;}
.rminfoBg72{ background: url(../newimages/info_bg72.png) no-repeat center center; text-align: center;}
.rminfoBg112{ background: url(../newimages/info_bg112.png) no-repeat center center; text-align: center;}
.rminfotextArea{ background: url(../newimages/textArea_bg312.png) no-repeat center center; padding: 0 8px;}
.rmInfoContent a{ color: #333; text-decoration: underline;}
.rmInfoContent a:hover{ color: #0078c8; text-decoration: none;}

.bTitle{ font-size: 18px; font-weight: bold;}
.searchPlan{ background: #eee url(../newimages/search_bg.png) repeat-x left top; border-bottom: 1px solid #b8b8b8; float: left; height: 35px; line-height: 35px; padding-top: 4px; width: 100%;}
