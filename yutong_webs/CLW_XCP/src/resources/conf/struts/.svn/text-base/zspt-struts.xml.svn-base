<struts>
	<package name="zspt" namespace="/zspt" extends="clw-default">		
		<!-- 专属平台－油量监控 -->
		<action name="zsptOil" class="zsptAction" method="zsptOil">
		   <result name="success">
				/WEB-INF/jsp/zspt/oilMonitor.jsp
			</result>
		</action>
		
		<!-- 专属平台－变动记录 -->
		<action name="zsptRecord" class="zsptAction" method="zsptRecord">
		   <result name="success">
				/WEB-INF/jsp/zspt/oilRecord.jsp
			</result>
		</action>
		
		<!-- 专属平台－站点设置 -->
		<action name="zsptSetStation" class="zsptAction" method="zsptSetStation">
		   <result name="success">
				/WEB-INF/jsp/zspt/setStation.jsp
			</result>
		</action>
		
