<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="alarmSettingAction" namespace="/alarmSetting" extends="clw-default json-default">	
		
		<action name="init" class="alarmSettingAction" >
			<result>/WEB-INF/jsp/sysmanage/sysset/alarmset/showsetting.jsp</result>	
		</action>
		<!-- 初始化列表 -->
		<action name="list" class="alarmSettingAction" method="list">
           <result name="success" type="json">
           	  <param name="contentType">text/json</param>
			  <param name="excludeNullProperties">false</param>
           	  <param name="noCache">true</param>
              <param name="root">map</param>
           </result>
           <result name="error" type="json">
              <param name="contentType">text/json</param>
              <param name="excludeNullProperties">false</param>
              <param name="noCache">true</param>
              <param name="root">map</param>
           </result>        
       </action>
       
		<!-- 初始化选中 -->
		<action name="checked" class="alarmSettingAction" method="checked">
           <result name="success" type="json">
           	  <param name="contentType">text/json</param>
			  <param name="excludeNullProperties">false</param>
           	  <param name="noCache">true</param>
              <param name="root">map</param>
           </result>
           <result name="error" type="json">
              <param name="contentType">text/json</param>
              <param name="excludeNullProperties">false</param>
              <param name="noCache">true</param>
              <param name="root">map</param>
           </result>        
       </action>
       
		<!-- 保存选中 -->
       <action name="set" class="alarmSettingAction" method="set">
           <result name="success" type="json">
           	  <param name="contentType">text/json</param>
			  <param name="excludeNullProperties">false</param>
           	  <param name="noCache">true</param>
              <param name="root">map</param>
           </result>
           <result name="error" type="json">
              <param name="contentType">text/json</param>
              <param name="excludeNullProperties">false</param>
              <param name="noCache">true</param>
              <param name="root">map</param>
           </result>        
       </action>
       
	</package>
</struts>